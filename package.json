{
  "name": "privata",
  "version": "1.0.0",
  "description": "Transparent GDPR/HIPAA compliance library with automatic data separation",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "clean": "nx reset && rm -rf node_modules packages/*/node_modules packages/*/dist",
    "dev": "nx run-many --target=dev --all --parallel",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@nx/js": "^17.0.0",
    "@nx/node": "^17.0.0",
    "@nx/workspace": "^17.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "express": "^5.1.0",
    "jest": "^29.5.0",
    "k6": "^0.0.0",
    "nx": "^17.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "gdpr",
    "hipaa",
    "compliance",
    "privacy",
    "data-separation",
    "pseudonymization",
    "audit-logging",
    "typescript",
    "orm"
  ],
  "author": "Privata Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/privata/privata.git"
  }
}
